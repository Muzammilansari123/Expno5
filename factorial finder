#include<stdio.h>
int factorialterative(int num) {
    int prod = 1;
    for (int i=1; i <=num; i++) {
        prod*=i;
    }
    return prod;
    }
    int main(){
        int num;
        printf("\t\t\t *** Iteractive Factorial Function *** \n\n");
        printf("Enter Number :");
        scanf("%d",&num);
        printf("Factorial of %d is %d",num, factorialterative(num));
        return 0;
    }



